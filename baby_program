import pandas as pd 

#Reads file
hunt = pd.read_excel('jbhunt.xlsx')

#Examine shape of data
hunt.shape

#Examine null cells
hunt.isnull

#View total of null values by column
hunt.isnull().sum()

#Remove all null
hunt=hunt.dropna()

#Store dataframe as new file
hunt.to_excel('jbhunt_new.xlsx',index=False)
